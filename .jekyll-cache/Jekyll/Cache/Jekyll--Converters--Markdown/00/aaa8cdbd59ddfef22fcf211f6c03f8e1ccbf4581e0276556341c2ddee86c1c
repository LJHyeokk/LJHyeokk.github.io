  <span class="p">}</span>
<span class="p">});</span>

<span class="kd">const</span> <span class="nx">comment</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">"</span><span class="s2">.comment</span><span class="dl">"</span><span class="p">);</span>
<span class="kd">const</span> <span class="nx">input</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">getElementById</span><span class="p">(</span><span class="dl">"</span><span class="s2">commentInput</span><span class="dl">"</span><span class="p">);</span>
<span class="c1">//ì—”í„°ì¹˜ë©´ ëŒ“ê¸€</span>
<span class="nx">input</span><span class="p">.</span><span class="nx">addEventListener</span><span class="p">(</span><span class="dl">"</span><span class="s2">keydown</span><span class="dl">"</span><span class="p">,</span> <span class="p">(</span><span class="nx">e</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
  <span class="k">if</span> <span class="p">(</span><span class="nx">e</span><span class="p">.</span><span class="nx">keyCode</span> <span class="o">===</span> <span class="mi">13</span><span class="p">)</span> <span class="p">{</span>
    <span class="c1">//ê°ì‹¸ëŠ” ë°•ìŠ¤ div</span>
    <span class="kd">const</span> <span class="nx">commentBox</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">createElement</span><span class="p">(</span><span class="dl">"</span><span class="s2">div</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">commentBox</span><span class="p">.</span><span class="nx">setAttribute</span><span class="p">(</span><span class="dl">"</span><span class="s2">class</span><span class="dl">"</span><span class="p">,</span> <span class="dl">"</span><span class="s2">commentBox</span><span class="dl">"</span><span class="p">);</span>
    <span class="c1">//í…ìŠ¤íŠ¸ ì¶”ê°€ span</span>
    <span class="kd">const</span> <span class="nx">commentText</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">createElement</span><span class="p">(</span><span class="dl">"</span><span class="s2">span</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">commentText</span><span class="p">.</span><span class="nx">setAttribute</span><span class="p">(</span><span class="dl">"</span><span class="s2">class</span><span class="dl">"</span><span class="p">,</span> <span class="dl">"</span><span class="s2">commentText</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">commentText</span><span class="p">.</span><span class="nx">innerHTML</span> <span class="o">=</span>
      <span class="dl">"</span><span class="s2">&lt;b&gt;</span><span class="dl">"</span> <span class="o">+</span> <span class="nx">sessionStorage</span><span class="p">.</span><span class="nx">getItem</span><span class="p">(</span><span class="dl">"</span><span class="s2">id</span><span class="dl">"</span><span class="p">)</span> <span class="o">+</span> <span class="dl">"</span><span class="s2">&lt;/b&gt;</span><span class="dl">"</span> <span class="o">+</span> <span class="dl">"</span><span class="s2"> : </span><span class="dl">"</span> <span class="o">+</span> <span class="nx">e</span><span class="p">.</span><span class="nx">target</span><span class="p">.</span><span class="nx">value</span><span class="p">;</span>
    <span class="c1">//ì¢‹ì•„ìš” span</span>
    <span class="kd">const</span> <span class="nx">likeBtn</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">createElement</span><span class="p">(</span><span class="dl">"</span><span class="s2">span</span><span class="dl">"</span><span class="p">);</span>
    <span class="kd">let</span> <span class="nx">likeNum</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
    <span class="nx">likeBtn</span><span class="p">.</span><span class="nx">setAttribute</span><span class="p">(</span><span class="dl">"</span><span class="s2">class</span><span class="dl">"</span><span class="p">,</span> <span class="dl">"</span><span class="s2">likeBtn</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">likeBtn</span><span class="p">.</span><span class="nx">innerHTML</span> <span class="o">=</span> <span class="s2">`  â¤ï¸ </span><span class="p">${</span><span class="nx">likeNum</span><span class="p">}</span><span class="s2">ëª…`</span><span class="p">;</span>
    <span class="nx">likeBtn</span><span class="p">.</span><span class="nx">addEventListener</span><span class="p">(</span><span class="dl">"</span><span class="s2">click</span><span class="dl">"</span><span class="p">,</span> <span class="p">()</span> <span class="o">=&gt;</span> <span class="p">{</span>
      <span class="nx">likeNum</span><span class="o">++</span><span class="p">;</span>
      <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">likeNum</span><span class="p">);</span>
      <span class="nx">likeBtn</span><span class="p">.</span><span class="nx">innerHTML</span> <span class="o">=</span> <span class="s2">` â¡ï¸ â¤ï¸ </span><span class="p">${</span><span class="nx">likeNum</span><span class="p">}</span><span class="s2">ëª…`</span><span class="p">;</span>
    <span class="p">});</span>
    <span class="c1">//ì‚­ì œ button</span>
    <span class="kd">const</span> <span class="nx">deleteBtn</span> <span class="o">=</span> <span class="nb">document</span><span class="p">.</span><span class="nx">createElement</span><span class="p">(</span><span class="dl">"</span><span class="s2">button</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">deleteBtn</span><span class="p">.</span><span class="nx">setAttribute</span><span class="p">(</span><span class="dl">"</span><span class="s2">class</span><span class="dl">"</span><span class="p">,</span> <span class="dl">"</span><span class="s2">deleteBtn</span><span class="dl">"</span><span class="p">);</span>
    <span class="nx">deleteBtn</span><span class="p">.</span><span class="nx">innerHTML</span> <span class="o">=</span> <span class="dl">"</span><span class="s2">ì‚­ì œ</span><span class="dl">"</span><span class="p">;</span>
    <span class="nx">deleteBtn</span><span class="p">.</span><span class="nx">addEventListener</span><span class="p">(</span><span class="dl">"</span><span class="s2">click</span><span class="dl">"</span><span class="p">,</span> <span class="p">()</span> <span class="o">=&gt;</span> <span class="p">{</span>
      <span class="nx">comment</span><span class="p">.</span><span class="nx">removeChild</span><span class="p">(</span><span class="nx">commentBox</span><span class="p">);</span>
    <span class="p">});</span>
    <span class="c1">//ì‹¤ì œë¡œ ë‹¬ê¸°</span>
    <span class="nx">commentText</span><span class="p">.</span><span class="nx">appendChild</span><span class="p">(</span><span class="nx">likeBtn</span><span class="p">);</span>
    <span class="nx">commentBox</span><span class="p">.</span><span class="nx">appendChild</span><span class="p">(</span><span class="nx">commentText</span><span class="p">);</span>
    <span class="nx">commentBox</span><span class="p">.</span><span class="nx">appendChild</span><span class="p">(</span><span class="nx">deleteBtn</span><span class="p">);</span>
    <span class="nx">comment</span><span class="p">.</span><span class="nx">appendChild</span><span class="p">(</span><span class="nx">commentBox</span><span class="p">);</span>

    <span class="nx">input</span><span class="p">.</span><span class="nx">value</span> <span class="o">=</span> <span class="dl">""</span><span class="p">;</span>
  <span class="p">}</span>
<span class="p">});</span>
</code></pre></div></div>

