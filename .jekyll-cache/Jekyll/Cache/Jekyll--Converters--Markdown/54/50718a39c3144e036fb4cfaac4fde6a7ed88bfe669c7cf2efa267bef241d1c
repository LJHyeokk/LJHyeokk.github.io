<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>const LinkedList = function () {
  this.head = null;
  this.size = 0;
};

const Node = function (data) {
  this.data = data;
  this.link = null;
};

LinkedList.prototype.add = function (data) {
  const newNode = new Node(data);
  if (this.head === null) {
    this.head = newNode; // Node { data: 5, link: null } // í—¤ë“œì— ì£¼ì†Œë„£ì–´ì£¼ê¸°
    this.size++;
  } else {
    let currentNode = this.head;
    while (currentNode.link !== null) {
      currentNode = currentNode.link;
    }
    currentNode.link = newNode;
    this.size++;
  }
};
// ìƒˆë¡œìš´ ë…¸ë“œë¥¼ ë§Œë“¤ê³  ìƒˆë¡œìš´ ë…¸ë“œì˜ ë§í¬ì—ëŠ” í—¤ë“œì˜ ê°’ì„ ë„£ì–´ì¤€ë‹¤. ê·¸ë¦¬ê³  í—¤ë“œì—ëŠ” ìƒˆë¡œìš´ ë…¸ë“œì˜ ê°’ì„ ë„£ì–´ì¤€ë‹¤
LinkedList.prototype.insertFirstNode = function (data) {
  const newNode = new Node(data);
  newNode.link = this.head;
  this.head = newNode;
  this.size++;
};

LinkedList.prototype.insertMiddleNode = function (data, index) {
  const newNode = new Node(data);
  let beforeNode = this.head;
  while (--index !== 0) {
    beforeNode = beforeNode.link;
  }
  let afterNode = beforeNode.link;
  beforeNode.link = newNode;
  newNode.link = afterNode;
  this.size++;
};

LinkedList.prototype.delete = function (index) {
  let beforeNode = this.head;
  while (--index !== 0) {
    beforeNode = beforeNode.link;
  }
  let tobeDelete = beforeNode.link;
  let afterNode = beforeNode.link.link;
  beforeNode.link = afterNode;
  delete tobeDelete;
  this.size--;
};

LinkedList.prototype.showData = function () {
  let currentNode = this.head;
  const result = Array(this.size).fill(0);
  for (let i = 0; i &lt; this.size; i++) {
    result[i] = currentNode.data;
    currentNode = currentNode.link;
  }
  return result;
};
